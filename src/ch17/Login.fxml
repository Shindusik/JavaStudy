<BorderPane fx:id="login" prefHeight="500.0" prefWidth="350.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18" fx:controller="ch17.LoginController">
   <top>
      <BorderPane style="-fx-background-color: #eaeaea;" BorderPane.alignment="CENTER">
         <left>
            <Button fx:id="btnMain" mnemonicParsing="false" text="메인" BorderPane.alignment="CENTER" />
         </left>
         <center>
            <Label alignment="CENTER" prefWidth="215.0" text="로그인 화면" BorderPane.alignment="CENTER" />
         </center>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </BorderPane>
   </top>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: #ffff00;" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
