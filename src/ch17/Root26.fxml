<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>


<HBox spacing="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="ch17.RootController26">
   <children>
      <Button mnemonicParsing="false" onAction="#handleOpenFileChooser" text="Open FileChooser" />
      <Button mnemonicParsing="false" onAction="#handleSaveFileChooser" text="Save FileChooser" />
      <Button mnemonicParsing="false" onAction="#handleDirectoryChooser" text="DirectorChooser" />
      <Button fx:id="btnPopup" mnemonicParsing="false" onAction="#handlePopup" text="Popup" />
      <Button fx:id="btnCustom" mnemonicParsing="false" onAction="#handleCustom" text="Custom">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
